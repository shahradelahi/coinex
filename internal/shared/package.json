{
  "name": "@coinex-org/internal-shared",
  "version": "0.0.0",
  "private": true,
  "description": "Shared types and enums for CoinEx API clients",
  "keywords": [
    "coinex",
    "shared",
    "enums",
    "types"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**",
    "!**/*.d.cts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf node_modules dist .tsbuildinfo",
    "dev": "tsup --watch",
    "format": "prettier --write ",
    "format:check": "prettier --check ",
    "lint": "pnpm typecheck && eslint ",
    "lint:fix": "eslint --fix ",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@coinex-org/eslint-config": "workspace:*",
    "@shahrad/tsconfig": "^1.0.0",
    "@types/node": "^22.13.14",
    "dotenv": "^17.2.1",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
